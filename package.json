{
  "name": "svelte-app",
  "description": "svelte and parcel from ground up",
  "version": "0.0.1",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "npm run clean && parcel serve src/index.html -p 8080 --log-level 4",
    "start": "npm run clean && parcel serve src/index.html -p 8080 --log-level 4",
    "build": "npm run clean && parcel build src/index.html --no-source-maps  --detailed-report"
  },
  "browserslist": [
    "last 1 chrome versions",
    "last 1 firefox versions",
    "not dead"
  ],
  "main": "dist/index.html",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@smui/button": "^1.0.0-beta.16",
    "@smui/common": "^1.0.0-beta.15",
    "@smui/drawer": "^1.0.0-beta.16",
    "@smui/icon-button": "^1.0.0-beta.16",
    "@smui/list": "^1.0.0-beta.16",
    "@smui/ripple": "^1.0.0-beta.16",
    "@smui/top-app-bar": "^1.0.0-beta.16",
    "cssnano": "^4.1.10",
    "eslint": "^6.6.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.12.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-html": "^0.2.1",
    "rollup-plugin-html-entry": "^0.2.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "sass": "^1.23.1",
    "svelte": "^3.12.1"
  },
  "dependencies": {
    "svero": "^1.0.0"
  }
}
