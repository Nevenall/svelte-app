{
  "name": "svelte-app",
  "description": "svelte and parcel from ground up",
  "version": "0.0.1",
  "scripts": {
    "start": "parcel serve src/index.html -p 8080 --log-level 4",
    "build": "parcel build --no-source-maps src/index.html --log-level 4 --detailed-report"
  },
  "browserslist": [
    "last 1 chrome versions",
    "last 1 firefox versions",
    "not dead"
  ],
  "main": "dist/index.html",
  "devDependencies": {
    "@smui/drawer": "^1.0.0-beta.14",
    "@smui/icon-button": "^1.0.0-beta.14",
    "@smui/list": "^1.0.0-beta.14",
    "@smui/ripple": "^1.0.0-beta.14",
    "@smui/top-app-bar": "^1.0.0-beta.14",
    "cssnano": "^4.1.10",
    "eslint": "^6.3.0",
    "parcel": "file:parcel",
    "parcel-plugin-svelte": "^4.0.4",
    "sass": "^1.23.0-module.beta.1",
    "svelte": "^3.12.1"
  }
}
