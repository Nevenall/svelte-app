{
  "name": "svelte-app",
  "description": "svelte and parcel from ground up",
  "version": "0.0.1",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "parcel serve src/index.html -p 8080 --log-level 4",
    "start": "npm run clean && parcel serve src/index.html -p 8080 --log-level 4",
    "build": "npm run clean && parcel build src/index.html --no-source-maps --log-level 4"
  },
  "source": "src/index.html",
  "main": "dist/index.html",
  "targets": {
    "main": {
      "browsers": [
        "last 2 version"
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@smui/button": "^1.0.0-beta.17",
    "@smui/common": "^1.0.0-beta.15",
    "@smui/drawer": "^1.0.0-beta.17",
    "@smui/icon-button": "^1.0.0-beta.17",
    "@smui/list": "^1.0.0-beta.17",
    "@smui/ripple": "^1.0.0-beta.17",
    "@smui/top-app-bar": "^1.0.0-beta.17",
    "abstract-nested-router": "^0.1.10",
    "cssnano": "^4.1.10",
    "eslint": "^6.5.1",
    "parcel": "^2.0.0-alpha.2.1",
    "parcel-transformer-svelte": "^1.0.0",
    "rimraf": "^3.0.0",
    "sass": "^1.23.3",
    "svelte": "^3.12.1",
    "typescript": "^3.6.4"
  },
  "dependencies": {},
  "alias": {}
}
